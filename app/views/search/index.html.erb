<h5>
  <%= "Stores within 25 miles of #{@presenter.zipcode}" %>
</h5>

<p>
  <%= "#{@presenter.total_stores} Total Stores" %>
</p>

<ul>
  <% @presenter.stores.each do |store| %>
    <div class="store">
      <li><%= #{store.name}  %></li>
      <li><%= store.city  %></li>
      <li><%= store.type  %></li>
      <li><%= store.distance %></li>
      <li><%= store.phone %></li>
    </div>
  <% end %>
</ul>
